import java.util.Scanner;

public class Main {
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        System.out.println("Digite o primeiro elemento:");
        int primeiroElemento = input.nextInt();
        System.out.println("Digite a razão:");
        int razao = input.nextInt();
        System.out.println("A progressão aritméica é");

        for(int i = 0; i<10; i++){
            int elemento = primeiroElemento + i* razao;
            System.out.print(elemento + " ");


        }



    }
}
